         <div class="inputBox"> 
   
          <input type="email" required name="email" maxlength="100"> <i>Email</i> 
   
         </div> 
   
         <div class="inputBox"> 
   
          <input type="password" required name="password" maxlength="100"> <i>Password</i> 
   
         </div> 
   
   
         <div class="inputBox"> 
   
          <input type="submit" value="Login"> 
   
         </div> 
         
         <h3>Are you new to Audioally? <a href="register">Register here</a></h3>
   
        </div> 
   
       </div> 
       <form action="" method="post" name="login">

      </div> 
   
     </section>


